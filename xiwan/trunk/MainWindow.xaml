<Window x:Class="Gqqnbig.TrafficVolumeCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:TrafficVolumeCalculator="clr-namespace:Gqqnbig.TrafficVolumeCalculator" mc:Ignorable="d" d:DesignHeight="200"
        Title="MainWindow" Loaded="Window_Loaded" Height="700">

    <StackPanel Name="grid">
        <TrafficVolumeCalculator:CaptureViewer x:Name="captureViewer1" />
        <TrafficVolumeCalculator:CaptureViewer x:Name="captureViewer2" />


        <StackPanel Grid.Row="0" Grid.RowSpan="2" Grid.Column="3">
            <Button Content="计算直方图相似度" Click="Button_Click" />
            <TextBox Name="histSimilarityTextBox" AcceptsReturn="True" IsReadOnly="True"/>
        </StackPanel>
    </StackPanel>
</Window>
